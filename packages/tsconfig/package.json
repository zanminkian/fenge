{
  "name": "@fenge/tsconfig",
  "version": "0.7.0",
  "description": "A strict sharable tsconfig preset.",
  "keywords": [
    "tsconfig",
    "strict",
    "out-of-box",
    "typescript",
    "ts",
    "config",
    "configuration"
  ],
  "homepage": "https://github.com/zanminkian/fenge/tree/main/packages/tsconfig",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zanminkian/fenge.git",
    "directory": "packages/tsconfig"
  },
  "license": "MIT",
  "author": "hellozmj@qq.com",
  "type": "module",
  "exports": {
    "./setup": "./src/setup.js",
    "./*.json": "./tsconfig/*.json",
    "./*": "./tsconfig/*.json",
    ".": "./tsconfig/esm.json"
  },
  "bin": {
    "tsconfig": "./src/tsconfig.cli.js"
  },
  "files": [
    "src",
    "tsconfig"
  ],
  "scripts": {
    "build": "tsc --noEmit"
  },
  "dependencies": {
    "commander": "13.1.0",
    "get-tsconfig": "4.10.1",
    "json5": "2.2.3",
    "print-diff": "2.0.0",
    "sort-keys": "5.1.0"
  },
  "devDependencies": {
    "@types/node": "20.19.11"
  },
  "peerDependencies": {
    "typescript": "^5.7.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.19.0"
  }
}

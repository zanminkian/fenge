{
  "name": "@git-validator/tsconfig",
  "version": "0.1.8",
  "description": "Strict shared tsconfig out-of-box.",
  "keywords": [
    "tsconfig",
    "strict",
    "out-of-box",
    "typescript",
    "ts",
    "config",
    "configuration"
  ],
  "homepage": "https://github.com/zanminkian/git-validator/tree/main/packages/tsconfig",
  "license": "MIT",
  "author": "hellozmj@qq.com",
  "type": "module",
  "exports": {
    "./cli": "./src/cli.js",
    "./cjs.json": "./cjs.json",
    "./esm.json": "./esm.json",
    "./legacy.json": "./legacy.json",
    "./tsconfig.json": "./tsconfig.json",
    "./cjs": "./cjs.json",
    "./esm": "./esm.json",
    "./legacy": "./legacy.json",
